{
    "name": "qobo/cakephp-menu",
    "description": "Menu plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "qobo/cakephp-utils": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Menu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Menu\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
